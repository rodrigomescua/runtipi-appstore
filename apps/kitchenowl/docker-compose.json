{
  "services": [
    {
      "name": "kitchenowl",
      "image": "tombursch/kitchenowl:v0.6.11",
      "isMain": true,
      "internalPort": 8080,
      "environment": {
        "TZ": "${TZ}",
        "JWT_SECRET_KEY": "${JWT_SECRET_KEY}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        }
      ]
    }
  ],
  "$schema": "../dynamic-compose-schema.json"
}
